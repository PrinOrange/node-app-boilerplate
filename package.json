{
  "name": "node-app-boilerplate",
  "version": "1.0.0",
  "description": "This is the boilerplate for nodejs with typescript, which is generated by Webpack. You could write your code here with needed dependencies and configurations.",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --config ./config/webpack.dev.config.ts",
    "build:prod": "webpack --config ./config/webpack.prod.config.ts",
    "build": "npm run build:dev & npm run build:prod",
    "dev": "webpack --config ./config/webpack.dev.config.ts --watch",
    "start": "npm run build:prod && node ./build/prod/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "npx ts-node ./scripts/clean.ts"
  },
  "keywords": [
    "nodejs",
    "webpack",
    "typescript",
    "boilerplate"
